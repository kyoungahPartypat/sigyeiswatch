header
  nav(id = "lnb" class = "navbar-default navbar-static-top nav-lnb")
    div.navbar-header
      button(type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target=".navbar-collapse" aria-expanded="false")
        span.icon-bar
        span.icon-bar
        span.icon-bar
    div(class = "navbar-custom collapse navbar-collapse")
      if (user)
        ul(class = "nav navbar-nav pull-right nav-lnb")
          li: a(href = "javascript:;" id = "myName" class = "name") #{user.name}
          li: a(href = "/users/logout") 로그아웃
      else
        ul(class = "nav navbar-nav pull-right nav-lnb")
          li: a(href = "/users/login") 로그인
          li: a(href = "/users/joinMenu") 회원가입
  h1: a(href = "/")
  nav#gnb
    ul(class = "nav nav-tabs nav-gnb")
      li.dropdown
        a(href = "#" class = "dropdown-toggle" data-toggle="dropdown") 킬링타임
        ul.dropdown-menu
          li: a(href = "/chat") 채팅
          li: a(href = "/game") 게임
      li: a(href = "/photos") 갤러리
      li.dropdown
        a(href = "/community" class = "dropdown-toggle" data-toggle="dropdown") 커뮤니티
        ul.dropdown-menu  
          li: a(href = "/free") 자유게시판

